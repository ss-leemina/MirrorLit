<!DOCTYPE html>
<html>

<head>
  <title>Article List</title>
</head>

<body>
  <h1>Articles</h1>
  <ul>
    <% articles.forEach(article=> { %>
      <a href="/articles/<%= article.article_id %>">
        <%= article.title %>
      </a>
      <img src="<%=article.image_url %>" alt=" 대체 사진">
      <li>
        <%= article.press %> |
          <%= new Date(article.created_at).toLocaleString('ko-KR') %>
      </li>
      <br>
      <hr>
      <% }) %>
  </ul>
</body>

</html>